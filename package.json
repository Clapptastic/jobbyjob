{
  "name": "clappcode",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "concurrently \"npm run dev:vite\" \"npm run dev:server\"",
    "dev:vite": "vite",
    "dev:server": "NODE_OPTIONS=\"--loader ts-node/esm\" ts-node-dev --respawn --transpile-only --project tsconfig.server.json --experimental-specifier-resolution=node src/server/index.ts",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:integration": "vitest run integration/",
    "test:ci": "vitest run && playwright test",
    "test:sprite:api": "testsprite run --mode api",
    "test:sprite:ui": "testsprite run --mode ui",
    "test:sprite:all": "testsprite run --mode all",
    "test:sprite:watch": "testsprite run --mode all --watch",
    "db:reset": "node scripts/manage-db.js",
    "db:verify": "node scripts/verify-db.js",
    "storage:setup": "node scripts/storage-setup.js",
    "functions:deploy": "node scripts/deploy-functions.js",
    "deploy": "npm run build && node scripts/deploy.js",
    "docker:build": "docker build -t clappcode .",
    "docker:run": "docker run -p 80:80 clappcode",
    "verify": "node scripts/verify-app.js",
    "clean": "node scripts/clean.js",
    "test:all": "npm run build:test-runner && node dist/test-runner.mjs",
    "build:test-runner": "node scripts/build-test-runner.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "2.47.16",
    "@tanstack/react-query": "5.64.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "cors": "^2.8.5",
    "date-fns": "^3.3.1",
    "express": "^4.18.2",
    "lucide-react": "^0.316.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-dropzone": "^14.2.3",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "6.28.2",
    "zod": "^3.22.4",
    "zustand": "4.5.6"
  },
  "devDependencies": {
    "@playwright/test": "^1.41.1",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "14.3.1",
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^8.22.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.2.1",
    "@vitest/ui": "^1.2.1",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "esbuild": "0.20.2",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^24.0.0",
    "msw": "^2.1.5",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "5.7.3",
    "vite": "5.4.11",
    "vite-plugin-pwa": "0.21.1",
    "vitest": "1.6.0",
    "workbox-cacheable-response": "7.3.0",
    "workbox-core": "7.3.0",
    "workbox-expiration": "7.3.0",
    "workbox-precaching": "7.3.0",
    "workbox-routing": "7.3.0",
    "workbox-strategies": "7.3.0",
    "workbox-window": "7.3.0"
  }
}
